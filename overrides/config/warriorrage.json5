{
	// Minimal kill count for rage to enable
	"minimalKillCount": 5,
	// In Seconds
	"rageDuration": 15,
	// Max kill count for rage bonus damage
	"maxKillCountCap": 100,
	// Number of kills, which will multiply the bonus damage eg. 5 means, every 5 kills attack damage will be increased by bonusDamageMultiplier value
	"killIntervalBetweenNextBonus": 2,
	// Bonus damage per 4 kills in a row
	"bonusDamageMultiplier": 1.0,
	// Enable Fire Damage on full kill count
	"enableFireDamage": false,
	// Render Rage Overlay on experience bar
	"renderRageOverlay": true,
	// Negative offsets to left side, positive to right
	"offsetX": 0,
	// Negative offsets to up, positive to down
	"offsetY": 0
}